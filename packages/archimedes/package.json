{
    "name": "archimedes",
    "version": "1.0.0",
    "description": "Bot that calculates damage, refund and stargen for enemies and servants from FGO (as well as provides some utilities) ",
    "main": "./dist/main.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "jsons": "cp src/assets/*.json dist/assets",
        "build": "tsc && npm run jsons",
        "start": "node --trace-warnings dist/main.js",
        "start-ts": "ts-node src/main.ts",
        "dev": "node --trace-warnings dist/main.js dev",
        "dev-ts": "ts-node src/main.ts dev"
    },
    "author": "Boy on the Beach",
    "license": "MIT",
    "engines": {
        "node": "18.x",
        "npm": "8.x"
    },
    "dependencies": {
        "@atlasacademy/api-connector": "^3.0.124",
        "discord.js": "^13.11.0",
        "dotenv": "^16.0.2",
        "fgo-calc": "^1.24.0",
        "fuse.js": "^6.6.2",
        "jsdom": "^19.0.0",
        "mathjs": "^10.6.4",
        "node-fetch": "^2.6.7"
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^3.3.0",
        "@types/jsdom": "^16.2.15",
        "@types/node": "^17.0.45",
        "typescript": "^4.8.3"
    }
}
